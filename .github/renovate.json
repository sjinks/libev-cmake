{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>sjinks/.github:renovate-config"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^README\\.md$"],
      "matchStrings": [
        "https:\\/\\/github\\.com\\/(?<depName>[^{}]*?)\\/releases\\/download\\/(?<currentValue>[^{}]*?)\\/([\\s]+.*EXPECTED_HASH SHA256=(?<currentHash>[a-fA-F0-9]+))?"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
